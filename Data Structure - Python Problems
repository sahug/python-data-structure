        "**Print Prime Numbers less than 20**"
      ],
      "metadata": {
        "id": "vypkhjWb6oFt"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def printPrimeNumbers(num: int):\n",
        "\n",
        "  prime = {2}\n",
        "\n",
        "  for i in range(2, num + 1):\n",
        "    if (i > 1):\n",
        "      # print(\"i \", i)\n",
        "      for j in range(2, i):\n",
        "        if (i % j) == 0:\n",
        "          # print(\"j \", j)\n",
        "          break\n",
        "      else:\n",
        "        prime.add(i)\n",
        "  \n",
        "  print(prime)\n",
        "\n",
        "printPrimeNumbers(50)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "NNps0xph6qw3",
        "outputId": "36122631-19cb-4ae6-c158-2cf8614f7e3a"
      },
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "{2, 3, 5, 37, 7, 41, 11, 43, 13, 47, 17, 19, 23, 29, 31}\n"
          ]
        }
      ]
