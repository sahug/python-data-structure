      "source": [
        "def fizzbuzz(i: int):\n",
        "\n",
        "  if i % 3 == 0 and i % 5 == 0:\n",
        "    return \"FizzBuzz\"\n",
        "  elif i % 3 == 0:\n",
        "    return \"FIzz\"\n",
        "  elif i % 5 == 0:\n",
        "    return \"Buzz\"\n",
        "\n",
        "fizzbuzz(9)"
